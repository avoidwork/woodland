{
  "name": "woodland",
  "version": "17.0.2",
  "description": "Lightweight HTTP router with automatic headers",
  "main": "dist/woodland.cjs",
  "module": "dist/woodland.esm.js",
  "type": "module",
  "sourceType": "module",
  "types": "types/woodland.d.ts",
  "files": [
    "dist",
    "tpl",
    "types"
  ],
  "scripts": {
    "benchmark": "node benchmark.js",
    "build": "mkdir -p dist && rm -rf dist/* && npm run rollup",
    "changelog": "auto-changelog -p",
    "coverage": "nyc npm run test",
    "fix": "eslint --fix *.js src/*.js test/*.js",
    "lint": "eslint *.js src/*.js test/*.js",
    "mocha": "mocha test/*.js",
    "rollup": "rollup --config",
    "sample": "node sample.js",
    "test": "npm run lint && npm run mocha",
    "types": "npx -p typescript tsc src/*.js --declaration --allowJs --emitDeclarationOnly --outDir types",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avoidwork/woodland.git"
  },
  "keywords": [
    "router",
    "http",
    "https",
    "routing",
    "api",
    "rest"
  ],
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/avoidwork/woodland/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/avoidwork/woodland",
  "bin": {
    "woodland": "cli.js"
  },
  "dependencies": {
    "mime-db": "^1.52.0",
    "precise": "^4.0.1",
    "tiny-coerce": "^3.0.0",
    "tiny-etag": "^4.0.1",
    "tiny-lru": "^11.2.2"
  },
  "devDependencies": {
    "auto-changelog": "^2.4.0",
    "autocannon": "^7.12.0",
    "concurrently": "^8.2.1",
    "deep-freeze": "^0.0.1",
    "eslint": "^8.50.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "rollup": "^4.0.0",
    "tiny-httptest": "^4.0.5",
    "typescript": "^5.2.2"
  }
}
