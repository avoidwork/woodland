{
  "name": "woodland",
  "version": "15.0.1",
  "description": "Lightweight HTTP router with automatic headers",
  "main": "index.js",
  "scripts": {
    "benchmark": "concurrently -k --success first \"node benchmarks\\woodland.js\" \"autocannon -c 100 -d 40 -p 10 localhost:8000\"",
    "benchmark-http": "concurrently -k --success first \"node benchmarks\\http.js\" \"autocannon -c 100 -d 40 -p 10 localhost:8001\"",
    "changelog": "auto-changelog -p",
    "test": "eslint *.js lib/*.js test/*.js && mocha test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/avoidwork/woodland.git"
  },
  "keywords": [
    "router",
    "http",
    "https",
    "routing",
    "api",
    "rest"
  ],
  "author": "Jason Mulligan <jason.mulligan@avoidwork.com>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/avoidwork/woodland/issues"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "http://avoidwork.github.io/woodland/",
  "bin": {
    "woodland": "cli.js"
  },
  "dependencies": {
    "mime-db": "^1.45.0",
    "precise": "^1.1.0",
    "tiny-coerce": "^1.1.1",
    "tiny-etag": "^2.1.1",
    "tiny-lru": "^7.0.6"
  },
  "devDependencies": {
    "auto-changelog": "^2.2.1",
    "autocannon": "^7.0.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.16.0",
    "mocha": "^8.2.1",
    "tiny-httptest": "^1.1.1"
  }
}
